{
  "name": "dominion",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "test": "jest",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "dev": "nodemon -w src -e ts --exec \"ts-node-transpile-only -r tsconfig-paths/register -r source-map-support/register src --watch\"",
    "dev:next": "next dev",
    "build": "ts-node-transpile-only -r tsconfig-paths/register -r source-map-support/register src",
    "start": "node dist"
  },
  "engines": {
    "node": "18",
    "pnpm": "8"
  },
  "dependencies": {
    "vality": "6.3.2",
    "ws": "8.13.0"
  },
  "devDependencies": {
    "@swc/core": "1.3.69",
    "@swc/jest": "0.2.26",
    "@total-typescript/ts-reset": "0.4.2",
    "@types/jest": "29.5.3",
    "@types/node": "20.3.1",
    "@types/ws": "8.5.5",
    "@typescript-eslint/eslint-plugin": "6.0.0",
    "@typescript-eslint/parser": "6.0.0",
    "eslint": "8.45.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "5.0.0",
    "jest": "29.6.1",
    "nodemon": "3.0.1",
    "prettier": "3.0.0",
    "source-map-support": "0.5.21",
    "ts-node": "10.9.1",
    "typescript": "5.1.6"
  }
}
